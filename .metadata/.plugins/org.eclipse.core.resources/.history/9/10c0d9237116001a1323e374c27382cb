package gamelevel;

public class BeginnerLevel extends PlayerLevel {

	@Override
	public void run() {
		
	}

	@Override
	public void jump() {
		System.out.println("초급자는 jump를 못합니다.");
	}

	@Override
	public void turn() {
		System.out.println("초급자는 turn을 못합니다.");
	}

	@Override
	public void shwoLevelMessage() {
		System.out.println("-----*초급자 레벨입니다.*-----");
	}

}
