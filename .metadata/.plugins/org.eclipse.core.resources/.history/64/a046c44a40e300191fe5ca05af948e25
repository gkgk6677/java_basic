package java_study;

public class Grandeur {
	//Class 力累 抗力 Grandeur
	public String color;
	public String gear;
	public int price;
	
	// 积己磊
	public Grandeur(){
		System.out.println("Grandeur constructor");
	}
	public void run() {
		System.out.println("--- run() ---");
	}
	public void stop() {
		System.out.println("--- stop() ---");
	}
}
