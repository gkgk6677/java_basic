package java_study;

public class conditional {

	public static void main(String[] args) {
		int arg1 = 20;
		int arg2 = 20;
		
		if(arg1 < arg2) {
			System.out.println("arg1은 arg2보다 작다.");
		} else if(arg1 > arg2){
			System.out.println("arg1은 arg2보다 크다.");
		} else {
			System.out.println("arg1와 arg2는 같다.");
		}
	}

}
